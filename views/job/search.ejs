                <% job.forEach(element => { %>
                <!-- Job Block -->
                <div class="job-block">
                  <div class="inner-box">
                    <div class="content">
                      <span class="company-logo"
                        ><img src="images/resource/company-logo/1-1.png" alt=""
                      /></span>
                      <h4>
                        <a href="/jobs/view/<%= element._id %>"
                          ><%= element.jobTitle %></a
                        >
                      </h4>
                      <ul class="job-info">
                        <li>
                          <span class="icon flaticon-briefcase"></span> <%=
                          element.company %>
                        </li>
                        <li>
                          <span class="icon flaticon-map-locator"></span>
                          <%= element.city %>
                        </li>

                        <!-- Calculate Time -->
                        <% const targetDate = new Date(element.createdAt); %> <%
                        const timeDifference = Date.now() -
                        targetDate.getTime(); %> <% let timeAgo = ""; %> <%
                        switch (true) { case timeDifference < 0: timeAgo = "";
                        break; case timeDifference < 1000 * 60: timeAgo = "Now";
                        break; case timeDifference < 1000 * 60 * 60: const
                        minutesAgo = Math.floor(timeDifference / (1000 * 60));
                        timeAgo = `${minutesAgo} minute${minutesAgo > 1 ? "s" :
                        ""} ago`; break; case timeDifference < 1000 * 60 * 60 *
                        24: const hoursAgo = Math.floor(timeDifference / (1000 *
                        60 * 60)); timeAgo = `${hoursAgo} hour${hoursAgo > 1 ?
                        "s" : ""} ago`; break; case timeDifference < 1000 * 60 *
                        60 * 24 * 7: const daysAgo = Math.floor(timeDifference /
                        (1000 * 60 * 60 * 24)); timeAgo = `${daysAgo}
                        day${daysAgo > 1 ? "s" : ""} ago`; break; default: const
                        weeksAgo = Math.floor(timeDifference / (1000 * 60 * 60 *
                        24 * 7)); timeAgo = `${weeksAgo} week${weeksAgo > 1 ?
                        "s" : ""} ago`; break; } %>
                        <!-- Calculate Time -->

                        <li>
                          <span class="icon flaticon-clock-3"></span> <%=
                          timeAgo %>
                        </li>
                        <li>
                          <span class="icon flaticon-money"></span> <%=
                          element.salary %>
                        </li>
                      </ul>
                      <ul class="job-other-info">
                        <li class="time"><%= element.jobType %></li>
                        <!-- <li class="privacy">Private</li>
                        <li class="required">Urgent</li> -->
                      </ul>
                      <button class="bookmark-btn">
                        <span class="flaticon-bookmark"></span>
                      </button>
                    </div>
                  </div>
                </div>
                <% }) %>

