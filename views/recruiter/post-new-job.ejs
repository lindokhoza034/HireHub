            <!-- <div class="text">Ready to jump back in?</div> -->
          </div>

          <div class="row">
            <div class="col-lg-12">
              <!-- Ls widget -->
              <div class="ls-widget">
                <div class="tabs-box">
                  <div class="widget-title">
                    <!-- <h4>Post Job</h4> -->
                  </div>

                  <div class="widget-content">
                    <!-- <div class="post-job-steps">
                      <div class="step">
                        <span class="icon flaticon-briefcase"></span>
                        <h5>Job Detail</h5>
                      </div>

                      <div class="step">
